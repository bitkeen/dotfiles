#!/usr/bin/env python3
# Get secondary monitor name.
# https://askubuntu.com/a/639501
#
# TODO: remove this script after testing `get-secondary-display-fast`.
import sys

# isort: off
import gi
gi.require_version("Gdk", "3.0")
from gi.repository import Gdk  # noqa, pylint: disable=wrong-import-position
# isort: on

gdkdsp = Gdk.Display.get_default()
try:
    print(gdkdsp.get_monitor(1).get_model())
except AttributeError:
    sys.exit(1)
